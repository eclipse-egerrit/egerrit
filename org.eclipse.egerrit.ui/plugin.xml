<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.egerrit.ui.editors.ChangeDetailEditor"
            default="false"
            extensions="mpe"
            icon="icons/EGerritcolor16.png"
            id="org.eclipse.egerrit.ui.editors.ChangeDetailEditor"
            name="ChangeDetail Editor">
      </editor>
   </extension>
 <extension
         point="org.eclipse.ui.commands">

      <command

            id="org.eclipse.egerrit.ui.editors.InvokeEditorCommand"
            name="editorCommand">
      </command>
</extension>
   <extension
         point="org.eclipse.ui.handlers">

      <handler
            class="org.eclipse.egerrit.ui.editors.EditorCommandHandler"
            commandId="org.eclipse.egerrit.ui.editors.InvokeEditorCommand">
      </handler>
</extension>   
 <extension

         point="org.eclipse.ui.bindings">

      <key

            commandId="org.eclipse.egerrit.ui.editors.InvokeEditorCommand"

            contextId="org.eclipse.ui.contexts.window"

            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"

            sequence="Alt+C">

      </key>
      </extension>
</plugin>
