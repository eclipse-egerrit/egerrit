Content-Type: text/ecl
Entry-Name: .content

with [get-view "Gerrit Dashboard (EGerrit)"] {
    with [get-combo -after [get-label "Total reviews:.*"]] {
        set-text "project:org.eclipse.egerrit.test"
    }
    get-button Search | click
    with [get-table] {
        get-cell 0 2 | click
        get-cell 0 2 | click
        get-cell 0 2 | mouse down -count 2
        double-click
        get-cell 0 2 | mouse up -count 2
    }
}
with [get-editor ".*"] {
    with [get-group General] {
        get-editbox -after [get-label "Topic:"] | set-text "This is a test topic"
        get-button Save | click
    }
    with [get-group Reviewers] {
        get-button Add | click
        with [get-editbox -after [get-label "Reviewer:"]] {
            set-text "test1@egerrit.eclipse.org"
        }
        get-button Add | click
    }
    get-tab-folder | get-tab-item Messages | click
    with [get-editbox -index 3] {
        click-text "2:1"
        key-type Up
        set-text "Test commit message HAS BEEN MODIFIED\n\n"
            + "Change-Id: I848d096ee804c47cd362d80b4eeead9fe40457a4\n"
    }
    get-button Save | click
}
with [get-editor "Test commit message HAS BEEN MODIFIED"] {
    with [get-tab-folder] {
        get-tab-item Files | click
        get-tab-item History | click
    }
    get-table | select ".*"
    get-button Refresh | click
    get-tab-folder | get-tab-item Summary | click
}
