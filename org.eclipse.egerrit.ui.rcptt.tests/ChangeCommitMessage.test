------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

//Create a review
runQueryAndOpenEditor [create-review -server $serverURL]

//Change the title of the review
with [get-editor] {
    get-tab-folder | get-tab-item Messages | click
    with [get-editbox -index 3] {
        set-text "Commit message from test by RCPTT\n\n"
            + "Change-Id: I508ea1bf110e80bd00611c61e59303351cd6f69b\n"
    }
    get-button Save | click
}

//Confirm that the subject has changed in the editor
get-editor | get-editbox -after [get-label "Subject:"] 
    | get-property text | equals "Commit message from test by RCPTT" | verify-true

//Confirm that the subject has changed in the dashboard
assertSubjectInDashboard "Commit message from test by RCPTT"
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
