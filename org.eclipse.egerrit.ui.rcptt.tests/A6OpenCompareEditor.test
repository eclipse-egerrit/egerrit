Content-Type: text/ecl
Entry-Name: .content

get-quick-access | set-text setupRCPTT | key-type Enter
with [get-view "Gerrit Dashboard (EGerrit)"] {
	get-table | get-column-header Updated | click | click
	get-combo | set-value "message:\"Test commit message number 1\""
	get-button -text Search | click
	with [get-table] {
		get-cell 0 2 | double-click
	}
}

//Open the compare editor against base
with [get-editor ".*Test commit message number 1"] {
	get-tab-folder | get-tab-item Files | click
	with [get-table ] {
		get-cell 0 2 | double-click
	}
}
try { get-window "EGerrit Tip" | get-button OK | click } -catch { } 
wait -ms 2000
//Add a comment
with [get-editor "A.java" | get-text-viewer -after [get-label "Patch Set 1: A.java"] -index 1] {
    set-caret-pos 1 26
    key-type Enter
    type-text abc
    key-type "M1+s"
}

//Open the compare editor against the workspace
with [get-editor ".*Test commit message number 1"] {
	get-tab-folder | get-tab-item Files | click
    get-combo -after [get-label "Diff against:"] | select Workspace
    with [get-table] {
        get-cell 0 2 | double-click
    }
}
try { get-window "EGerrit Tip" | get-button OK | click } -catch { } 

