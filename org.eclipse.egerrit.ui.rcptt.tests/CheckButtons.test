Check the default enablement state of the buttons in the editor
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

//Create a review and open it
runQueryAndOpenEditor [create-review -server $serverURL]

with [get-editor] {
	get-button Refresh | get-property enablement | equals true | verify-true
	get-button Submit | get-property enablement | equals false | verify-true
	get-button Abandon | get-property enablement | equals true | verify-true
	get-button Rebase | get-property enablement | equals true | verify-true
	get-button "Cherry-Pick" | get-property enablement | equals true | verify-true
	get-button "Reply ..." | get-property enablement | equals true | verify-true
}


